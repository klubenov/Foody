<!-- Menu Toggle Script -->

@section Scripts
    {
    <script>
    $("#menu-toggle").click(function (e) {
        e.preventDefault();
        $("#wrapper").toggleClass("toggled");
    });</script>
    <script type="text/javascript">
        $("#loadPartialView").click(function () {
            $.get('@Url.Action("Menu","Users", new { Area = "Administration" })', {}, function (response) {
                $("#Display").html(response);
            });
        });
    </script>
}

